schema User {
  id: int;
  name: string;
  age: int;
  status: string;
}

source users <- File(path: "examples/users.csv") using User;
sink final_report -> File(path: "examples/final_report.csv");

active_users = users -> filter(user.status == 'active');

active_users -> select(id, name) -> final_report;